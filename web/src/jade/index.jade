    script(src='index.js')
    link(rel='stylesheet', href='index.css')
  body
    .navbar.navbar-inverse.navbar-fixed-top(role='navigation')
      .container
        .navbar-header
          button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='.navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='#') Knottie Example
        .collapse.navbar-collapse
          ul.nav.navbar-nav
            li.active
              a(href='#')
                span.fa.fa-home
                | Home
            li
              a(href='#about')
                span.fa.fa-child
                | About
            li
              a(href='#contact')
                span.fa.fa-mobile
                | Contact
    include home.jade
